 #include<stdio.h>

struct employee

{

    int id,age,salary;
long phonenumber;

    char name[25];

}
emp[100];
int main()

{

    int i,n;

    printf("enter the no of employees\n");

    scanf("%d",&n);

    printf("enter employee info as id , name , age , salary,phonenumber\n");

    for(i=0;i<n;i++)

    {

        scanf("%d %s %d %d %ld",&emp[i].id,&emp[i].name,&emp[i].age,&emp[i].salary,&emp[i].phonenumber);

    }

    printf("\nemp_name\temp_name\temp_age\t\temp_sal\temp_phonenumber\n");

    for(i=0;i<n;i++)

    {

        printf("%d\t\t%s\t\t%d\t\t%d\n",emp[i].id,emp[i].name,emp[i].age,emp[i].salary);

    }

}


